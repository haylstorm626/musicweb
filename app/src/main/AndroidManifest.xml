<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.capstone.musicweb">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Music Web"
        android:supportsRtl="false"
        android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar"
        >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyListActivity" />
        <activity android:name=".ArtistActivity" />
        <activity android:name=".AlbumActivity" />
        <activity android:name=".WebActivity" />
        <activity android:name=".TrackAdapter" />
        <activity android:name=".AlbumAdapter" />
        <activity android:name=".AlbumTrackAdapter" />
        <activity android:name=".ArtistAdapter" />

        <service android:name=".PlayerService" />

        <activity android:name=".MyListAdapter" />
        <activity android:name=".WebView"></activity>
    </application>

</manifest>